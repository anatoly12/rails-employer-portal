<div class="bg-white text-black shadow-2xl p-4 md:max-w-xl md:mx-auto md:p-8 lg:max-w-2xl lg:p-12">
  <h2 class="text-3xl font-medium mb-8">
    <%= link_to employee_path(search.employee_editor), class: "float-right -mt-2 -mr-2 hover:text-blue-500 hover:border-blue-500 h-10 w-10 p-2" do %>
      <%= render "icon/close_outline", icon_class: "pointer-events-none" %>
    <% end %>
    Symptom log <span class="font-light">from <%= search.date %></span>
  </h2>
  <ul id="entries" class="text-xl font-thin">
    <% search.results.each_with_index do |entry, index| %>
      <%= render "symptom_logs/entry", entry: entry, index: index %>
    <% end %>
  </ul>
</div>
